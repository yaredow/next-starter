{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:all": "concurrently \"pnpm dev --turbopack\" \"pnpm run dev:stripe\"",
    "dev:stripe": "stripe listen --forward-to localhost:3000/api/auth/stripe/webhook",
    "build": "next build",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "concurrently -k -s first \"pnpm dev\" \"sleep 10 && pnpm cy:run\"",
    "test:e2e:open": "concurrently -k \"pnpm dev\" \"sleep 10 && pnpm cy:open\"",
    "test:e2e:ci": "concurrently -k -s first \"pnpm dev\" \"sleep 10 && cypress run\"",
    "test:all": "pnpm test && pnpm test:e2e",
    "lint": "eslint .",
    "email:dev": "email dev --dir src/emails",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "uc:check": "ultracite check",
    "uc:fix": "ultracite fix"
  },
